import java.util.Arrays;
import java.util.Scanner;

public class Equal_median 
{
	/*int fn()
    {
        if(a[mid]==b[mid])
        return count;
        else
        {
           temp[]=a[mid];
           a[mid]=b[mid];
           b[mid]=temp[];
           
           Arrays.sort(a);
           Arrays.sort(b);
           count++;
           fn();
            
        }
        
        
